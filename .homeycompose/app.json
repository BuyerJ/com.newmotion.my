{
  "id": "com.newmotion.my",
  "name": {
    "en": "My New Motion"
  },
  "sdk": 2,
  "brandColor": "#000000",
  "version": "2.2.1",
  "compatibility": ">=1.5.0",
  "permissions": [
    "homey:manager:geolocation"
  ],
  "author": {
    "name": "Kaoh",
    "email": "kaoh@kaoh.nl"
  },
  "contributing": {
    "donate": {}
  },
  "category": "energy",
  "tags": {
    "en": [
      "Charging",
      "EV",
      "Electric car"
    ],
    "nl": [
      "Laadpaal",
      "Laden",
      "EV",
      "Elektrische auto"
    ]
  },
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "description": {
    "en": "Support for private new motion charging points.",
    "nl": "Ondersteuning voor private new motion laadpunten"
  },
  "capabilities": [
    "onoff",
    "measure_power",
    "alarm_online"
  ],
  "capabilitiesOptions": {
    "measure_power": {
      "approximated": true
    }
  },
  "settings": [
    {
      "type": "group",
      "label": {
        "en": "Account settings"
      },
      "children": [
        {
          "id": "user_email",
          "type": "text",
          "label": {
            "en": "E-mail"
          },
          "value": "E-Mail",
          "hint": {
            "en": "Your My New Motion account E-Mail"
          }
        },
        {
          "id": "user_password",
          "type": "password",
          "label": {
            "en": "Your My New Motion password"
          }
        }
      ]
    }
  ]
}